<div class="dash-home">
<!--   <div> -->
    <div class="container padded">
      <div class="page_content">
        <div class="row">
          <div class="col-xs-12 col-md-6 col-md-offset-3">

            <div class="row text-center padded">
              <% @candidates.each do |candidate| %>
                <%if candidate.picture_file_size.nil?%>
                  <p><%= candidate.firstname %> <%= candidate.lastname %> <%= link_to "edit", edit_candidate_path(candidate), class: "navbar-wagon-button btn" %></p>
                <%else%>
                  <p><%= image_tag candidate.picture.url(:medium), class: "img-circle" %> <%= link_to "edit", edit_candidate_path(candidate), class: "navbar-wagon-button btn" %></p>
                <%end%>
              <% end %>
            </div>


          </div>
        </div>
      </div>
    </div>
</div>
